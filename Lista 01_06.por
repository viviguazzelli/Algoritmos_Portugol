	funcao inicio()
	{
		cadeia nome, sobrenome
		escreva("Digite seu nome: ")
		leia (nome)
		escreva ("Digite seu sobrenome: ")
		leia(sobrenome)
		escreva ("\nSeja bem-vindo(a), ",nome," ", sobrenome,"\n")
	}
}
